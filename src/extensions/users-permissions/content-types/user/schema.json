{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "freeRecipesLeft": {
      "type": "integer",
      "default": 50,
      "max": 2000
    },
    "userdata": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::userdata.userdata",
      "inversedBy": "owner"
    },
    "userrecipe": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::userrecipe.userrecipe",
      "mappedBy": "user"
    },
    "userFavorites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::userfavorite.userfavorite",
      "mappedBy": "user"
    },
    "stripeCustomerId": {
      "type": "string"
    },
    "freeAccount": {
      "type": "boolean",
      "default": true
    },
    "paidMembershipTierOne": {
      "type": "boolean",
      "default": false
    },
    "paidMembershipTierTwo": {
      "type": "boolean",
      "default": false
    },
    "paidMembershipTierThree": {
      "type": "boolean",
      "default": false
    },
    "fullName": {
      "type": "string"
    }
  }
}
